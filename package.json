{
  "name": "eyes-of-hell",
  "version": "0.1.0",
  "description": "A game where you walk around and look at things",
  "main": "index.js",
  "scripts": {
    "build": "wasm-pack --verbose build --target browser --mode normal --dev --features web_debug && webpack",
    "release": "wasm-pack --verbose build --target browser --mode normal --release && webpack",
    "serve": "webpack serve",
    "try": "pnpm build && pnpm serve"
  },
  "keywords": [],
  "author": "",
  "license": "GPLv3.0-or-later",
  "packageManager": "pnpm@10.5.2",
  "devDependencies": {
    "@types/node": "^22.13.8",
    "@types/webpack": "^5.28.5",
    "copy-webpack-plugin": "^13.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "wasm-pack": "^0.13.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "wasm-pack"
    ]
  }
}
